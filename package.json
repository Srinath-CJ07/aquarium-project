{
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.17",
    "prettier": "^3.6.2",
    "prettier-plugin-go-template": "^0.0.15",
    "tailwindcss": "^4.1.17"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-go-template"
    ],
    "overrides": [
      {
        "files": [
          "*.html"
        ],
        "options": {
          "parser": "go-template"
        }
      }
    ]
  }
}
